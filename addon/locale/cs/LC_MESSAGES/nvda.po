#: addon\appModules\totalcmd.py:75 addon\appModules\totalcmd.py:88
msgid "No size information. Try select this item."
msgstr "Informace o velikosti není k dispozici. Zkuste vybrat položku."

#: addon\appModules\totalcmd.py:81 addon\appModules\totalcmd.py:111
msgid "The size is calculated, wait a few seconds..."
msgstr "Probíhá výpočet velikosti. Prosím čekejte."

#: addon\appModules\totalcmd.py:117 addon\appModules\totalcmd.py:121
msgid "There is no information about the time of the change."
msgstr "Informace o čase poslední změny není k dispozici."

#: addon\appModules\totalcmd.py:128
#, python-brace-format
msgid "Selected {count} items"
msgstr "Vybráno {count} položek"

#. Translators: this message is spoken when the user manually deselects items.
#: addon\appModules\totalcmd.py:132
msgid "Nothing selected"
msgstr "Nic nevybráno"

#: addon\appModules\totalcmd.py:140
#, python-brace-format
msgid "Selected {selected} of {total} items"
msgstr "Vybráno {selected} z {total} položek"

#: addon\appModules\totalcmd.py:143 addon\appModules\totalcmd.py:333
#: addon\appModules\totalcmd.py:345 addon\appModules\totalcmd.py:355
